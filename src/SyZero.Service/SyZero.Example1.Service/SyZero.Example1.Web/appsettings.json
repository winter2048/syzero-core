{
  "SyZero": {
    "Name": "SyZero.Example1", //服务名称
    "Port": "5002", //端口号
    "Ip": "", //内网Ip
    "WanIp": "", //广域网Ip
    "Protocol": "HTTP", //协议 HTTP/HTTPS/GRPC 
    "InspectInterval": 5, //健康检查间隔 秒
    "CorsOrigins": "http://localhost:4200,http://localhost:8080,http://localhost:8081,http://localhost:3000",
    "DynamicConfig": "" //动态配置(从consul/nacos中获取) 默认服务名 
  },
  "ConnectionString": {
    "Master": "Data Source=app.db", //主库
    "Slave": [
      {
        "ConnectionString": "Data Source=app.db",
        "HitRate": 1
      }
    ],
    "Type": "Sqlite" //MySql|SqlServer|Sqlite|Oracle|PostgreSQL|Dm|Kdbndp
  },
  "Redis": {
    "Master": "127.0.0.1:6379", //主
    "Slave": [ "127.0.0.1:6379" ], //从
    "Sentinel": [ "127.0.0.1:2673", "127.0.0.1:2673" ], //哨兵
    "Type": "MasterSlave" //MasterSlave|Sentinel|Cluster
  },
  "App": {
    "ServerRootAddress": "http://SyZero.Example1.com",
    "ClientRootAddress": "http://SyZero.Example1.com",
    "CorsOrigins": "http://localhost:4200,http://localhost:8080,http://localhost:8081,http://localhost:3000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "JWT": {
    "SecurityKey": "123456789123456789",
    "expires": "7",
    "issuer": "SyZero.Example1.com",
    "audience": "SyZero.Example1.com"
  },
  "AllowedHosts": "*",
  "Consul": {
    "ConsulAddress": "http://127.0.0.1:8500", //consul 服务器地址
    "HealthCheck": "/Health", //健康检查地址
    "Token": "123456"
  },
  "Nacos": {
    "ServerAddresses": [ "http://localhost:8848/" ],
    "Namespace": "public",
    "UserName": "nacos",
    "Password": "nacos",
    "ConfigUseRpc": false,
    "NamingUseRpc": false
  },
  "OpenTelemetry": {
    "JaegerUri": "http://localhost:14268/api/traces"
  }
}
